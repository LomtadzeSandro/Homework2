<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListFragment">

    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="List Fragment"
        android:textSize="25sp"
        android:textColor="@color/black"

        app:layout_constraintBottom_toTopOf="@+id/toProfileIV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/toProfileIV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_testing"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/toHomeIV"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go To Profile Page"
        android:textColor="@color/black"

        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="@id/toProfileIV"
        app:layout_constraintEnd_toEndOf="@id/toProfileIV"
        app:layout_constraintTop_toBottomOf="@id/toProfileIV"
        />

    <ImageView
        android:id="@+id/toHomeIV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_testing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/toProfileIV"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go To Home Page"
        android:textColor="@color/black"

        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="@id/toHomeIV"
        app:layout_constraintEnd_toEndOf="@id/toHomeIV"
        app:layout_constraintTop_toBottomOf="@id/toHomeIV"
        />

</androidx.constraintlayout.widget.ConstraintLayout>